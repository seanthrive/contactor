<%= form_with scope: :user, url: users_path, local: true do |form| %>
	<h2>Create an account!</h2>

	<p>
		<%= form.label :username %><br>
		<%= form.text_field :username %>
	</p>

	<p>
		<%= form.label :password %><br>
		<%= form.text_field :password %>
	</p>

	<p>
		<%= form.submit %>
	</p>
<% end %>